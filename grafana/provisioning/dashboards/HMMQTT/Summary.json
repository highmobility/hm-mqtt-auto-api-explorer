          "query": "treshould = from(bucket: \"something\")\r\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"mqtt_consumer\" and r[\"_field\"] == \"maintenance_service_distance_threshold_data_value\" ) \r\n  |> map(fn: (r) => ({ r with currentStatus: if float(v: r._value) == float(v: 0) then 1 else  0 }))\r\n  |> filter(fn: (r) => r.currentStatus == 1)\r\n  |> unique(column: \"topic_vin\")\r\n  |> drop(columns: [\"_field\", \"_value\"])\r\n  |> group(columns: [\"currentStatus\"])\r\n\r\nserviceDate = from(bucket: \"something\")\r\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"mqtt_consumer\" and r[\"_field\"] == \"maintenance_next_inspection_date_data\" ) \r\n  |> map(fn: (r) => ({ r with currentStatus: if (uint(v:uint(v: time(v: r._value ))) > uint(v: now())) then 1 else 0  }))\r\n  |> filter(fn: (r) => r.currentStatus == 1) \r\n  |> unique(column: \"topic_vin\")\r\n  |> drop(columns: [\"_field\", \"_value\"])\r\n  |> group(columns: [\"currentStatus\"])\r\n\r\ntabel = union(tables: [treshould, serviceDate])\r\n  |> unique(column: \"topic_vin\")\r\n  |> group()\r\n  |> count(column: \"currentStatus\")\r\n  |> map(fn: (r) => ({ r with _time: now(),_value: r.currentStatus  }))\r\n  |> yield(name: \"service\")\r\n\r\ncount = from(bucket: \"something\")\r\n  // range start is fixed , else it will load only selected range devices\r\n  |> range(start: 2010-05-22T23:30:00Z, stop: v.timeRangeStop) \r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"dashboard_lights\" or r[\"_measurement\"] == \"diagnostics\")\r\n  |> unique(column: \"topic_vin\")\r\n  |> filter(fn: (r) => r[\"_field\"] == \"diagnostics_odometer_data_value\" )\r\n  |> aggregateWindow(every: v.windowPeriod, fn: last, createEmpty: false)\r\n  |> group()\r\n  |> count(column: \"topic_vin\")\r\n  |> map(fn: (r) => ({ r with _time: now(),_value: r.topic_vin  }))\r\n  |> yield(name: \"_table\")\r\n\r\n\r\njoin(tables: {key1: tabel, key2: count}, on: [\"_time\"], method: \"inner\")\r\n  |> map(fn: (r) => ({  r with service_vehicles_percent: (float(v: r.currentStatus) * float(v: 100) /  float(v: r.topic_vin))   }))\r\n  |> drop(columns:[\"_field\", \"_value_key1\", \"_value_key2\", \"currentStatus\", \"topic_vin\", \"_time\"])\r\n  |> yield(name: \"end\")\r\n ",
